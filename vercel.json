{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        { "src": "/resources/(.*)", "dest": "/public/resources/$1" },
        { "src": "/favicon/(.*)", "dest": "/public/favicon/$1" },
        { "src": "/image/(.*)", "dest": "/public/image/$1" },
        { "src": "/css/(.*)", "dest": "/public/css/$1" },
        { "src": "/js/(.*)", "dest": "/public/js/$1" },
        { "src": "/fonts/(.*)", "dest": "/public/fonts/$1" },
        { "src": "/videos/(.*)", "dest": "/public/videos/$1" },
        { "src": "/audio/(.*)", "dest": "/public/audio/$1" },
        { "src": "/docs/(.*)", "dest": "/public/docs/$1" },
        { "src": "/excel/(.*)", "dest": "/public/excel/$1" },
        { "src": "/pdf/(.*)", "dest": "/public/pdf/$1" },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://bpsdm-sultra-lms-main-web.vercel.app",
        "APP_KEY": "base64:+9s/TnUQlHKGjmNRO2uaugb8+S7actweICLQWlqa9hI=",

        "FILESYSTEM_ROOT": "/tmp",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}